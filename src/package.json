  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Solcany/multi-timeline-horizontal-scrolling"
  },
  "homepage": "",
  "devDependencies": {
    "concurrently": "^3.4.0"
  },
  "dependencies": {
    "npm": "^6.9.0"
  }
}
